@using DonerSD.Models;
@model IEnumerable<ProductModel>
@{
    ViewBag.Title = "List";
}
<div class="container" pb-5>
    <div class="row">
        <div class="col-md-3">
            @Html.Action("GetCategories","Home")
        </div>
        <div class="col-md-9">

            @if (Model.Count() == 0)
            {
                <div class="alert alert-warning" role="alert">
                    
                        <i class="fa fa-exclamation" aria-hidden="true"></i>Intenging i denne kategorien!
                    
                </div>
            }
            else
            {
        <div class="alert alert-info" role="alert">
            <div>

                <i class="fa fa-check-square" aria-hidden="true"></i> 
                <strong>@Model.Count()</strong> vare(r) funnet!
            </div>
        </div>
            }

            @Html.Partial("_ProductList", Model)
        </div>
    </div>
</div>
